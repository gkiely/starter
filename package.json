{
  "name": "Starter",
  "version": "1.0.0",
  "description": "Starter for front end projects",
  "main": "index.js",
  "scripts": {
    "build:dev": "gulp dev",
    "build:prod": "cross-env NODE_ENV=production gulp prod",
    "clean": "rimraf dist",
    "dll:dev": "webpack --config config/webpack.dev.dll.js",
    "dll:prod": "cross-env NODE_ENV=production webpack --config config/webpack.prod.dll.js",
    "js:dev": "webpack --config config/webpack.dev.js --hide-modules",
    "js:prod": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js --hide-modules",
    "libsass": "rebuild-node-sass",
    "lint:fix": "eslint src/js --fix",
    "lint:dev": "eslint src/js --cache",
    "lint:prod": "eslint src/js",
    "setup": "yarn || (npm i yarn -g && yarn)",
    "test:dev": "jest -o",
    "test:prod": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "npm run setup && npm-run-all clean --parallel lint:dev libsass dll:dev --parallel js:dev test:dev --sequential build:dev",
    "production": "npm run setup && npm-run-all clean lint:prod libsass dll:prod js:prod test:prod build:prod",
    "now-start": "node index",
    "test-prod": "NODE_ENV=production node index"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "eslint": "^4.12.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "7.5.1",
    "get-my-ip": "0.0.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-eslint": "4.0.0",
    "gulp-file-include": "1.2.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "2.0.2",
    "gulp-livereload": "3.8.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-noop": "^1.0.0",
    "gulp-processhtml": "^1.1.0",
    "gulp-sass": "3.1.0",
    "gulp-sass-glob": "1.0.8",
    "gulp-sequence": "^0.4.6",
    "gulp-sourcemaps": "2.6.1",
    "npm-run-all": "^4.0.2",
    "rebuild-node-sass": "^1.1.0",
    "rimraf": "^2.6.1",
    "serve-index": "^1.9.1",
    "style-loader": "^0.23.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.6",
    "connect-pg-simple": "^4.2.1",
    "cookie-parser": "^1.4.3",
    "create-react-class": "^15.6.2",
    "cross-env": "^5.1.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "firebase": "^5.5.3",
    "firebase-admin": "^6.0.0",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "node-cache": "^4.2.0",
    "pg-promise": "^7.3.2",
    "pwd": "^1.1.0",
    "react": "^16.1.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "store": "^2.0.12"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "gkiely",
  "license": "ISC",
  "homepage": ""
}
